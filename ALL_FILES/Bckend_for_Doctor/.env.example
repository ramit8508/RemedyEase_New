# Environment variables for Doctor Backend

# Database
DATABASE_URI=your_mongodb_connection_string_for_doctors

# Server Configuration
PORT=5001
NODE_ENV=development

# Other Backend URLs (for inter-service communication)
USER_BACKEND_URL=http://localhost:8000

# CORS Origins (for production, replace with actual frontend URL)
CORS_ORIGIN=http://localhost:5173

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# Cloudinary Configuration (if used)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# AI Service Configuration
GROQ_API_KEY=your_groq_api_key_here